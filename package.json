{
  "name": "python-function-analyzer",
  "displayName": "Python Function Analyzer",
  "description": "Analyzes Python files for function counts.",
  "version": "0.0.1",
  "publisher": "your-name-or-id",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.analyzePythonFunctions"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.analyzePythonFunctions",
        "title": "Analyze Python Function Count"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/vscode": "^1.78.0",
    "typescript": "^5.3.3",
    "vscode": "^1.1.41"
  }
}
